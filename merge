# IPython log file

import pandas as pd
import os

files = [f for f in os.listdir('.') if f.endswith('.csv')]
dfs = []

for file in files:
	with open(file, 'r', encoding = 'cp949', errors = "ignore") as f:
		df = pd.read_csv(f, dtype={4:str})
		dfs.append(df)
final_df = pd.concat(dfs, ignore_index = True)
final_df.to_csv('merged0110_kr.csv', index=False, encoding = 'cp949')

